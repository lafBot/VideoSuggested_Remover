{
    "manifest_version": 2,
    "name": "Video Suggested Content Remover",
    "version": "1.0",
    "permissions": [
        "activeTab"
    ],
    "content_scripts": [
        {
            "matches": [
                "https://www.youtube.com/*",
                "https://www.springboard.com/*"
            ],
            "js": [
                "app.js"
            ]
        }
    ]
}